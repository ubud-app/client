{
	"name": "@dwimm/client-web",
	"description": "DWIMM Web Client",
	"scripts": {
		"check": "npm run check:eslint",
		"check:eslint": "eslint ./",
		"visualize": "plato -rt ical-generator -d ./code-visualization -e \".eslintrc.json\" ./app/js/**/*.js",
		"build": "npm run build:production",
		"build:production": "PRODUCTION=1 grunt --production",
		"build:dev": "grunt dev",
		"pipeline": "npm run check && npm run visualize && npm run build",
		"bump:reset": "export MODULEVERSION=$(npm view @dwimm/client-web version) && npm run bump:reset:package && npm run bump:reset:lock",
		"bump:reset:next": "export MODULEVERSION=$(npm view @dwimm/client-web@next version) && npm run bump:reset:package && npm run bump:reset:lock",
		"bump:reset:package": "cat ./package.json | jq -S \".version = \\\"${MODULEVERSION}\\\"\" > ./package.json",
		"bump:reset:lock": "cat ./package-lock.json | jq -S \".version = \\\"${MODULEVERSION}\\\"\" > ./package-lock.json",
		"bump:develop": "npm run bump:reset:next && npm --no-git-tag-version version prerelease && npm publish --tag next",
		"bump:production": "npm run bump:reset && npm --no-git-tag-version version from-git && npm publish"
	},
	"author": "Sebastian Pekarek <bugs@sebbo.net>",
	"devDependencies": {
		"babel-core": "^6.26.0",
		"babel-loader": "^7.1.2",
		"babel-polyfill": "^6.26.0",
		"babel-preset-env": "^1.6.0",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-stage-0": "^6.24.1",
		"babelify": "^7.3.0",
		"eslint": "^4.10.0",
		"eslint-plugin-security": "^1.4.0",
		"grunt": "^1.0.1",
		"grunt-browser-sync": "^2.2.0",
		"grunt-browserify": "^5.2.0",
		"grunt-contrib-clean": "^1.1.0",
		"grunt-contrib-copy": "^1.0.0",
		"grunt-contrib-cssmin": "^1.0.2",
		"grunt-contrib-htmlmin": "^2.4.0",
		"grunt-contrib-watch": "^1.0.0",
		"grunt-replace": "^1.0.1",
		"grunt-sass": "^1.2.1",
		"handlebars": "^4.0.10",
		"hbsfy": "^2.8.1",
		"plato": "^1.7.0",
		"stringify": "^5.1.0",
		"uglifyify": "^4.0.4"
	},
	"dependencies": {
		"backbone": "^1.3.3",
		"counterpart": "^0.18.3",
		"gravatar-url": "^2.0.0",
		"jquery": "^3.2.1",
		"moment": "^2.19.1",
		"numbro": "^1.11.0",
		"raven-js": "^3.18.1",
		"socket.io-client": "^2.0.3",
		"underscore": "^1.8.3"
	}
}
